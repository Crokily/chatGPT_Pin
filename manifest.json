{
  "manifest_version": 3,
  "name": "ChatGPT Pin",
  "version": "1.0",
  "description": "Enable pinning of multiple chats to the GPT page.",
  "icons": {
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png",
    "372": "icon/372.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*","https://chatgpt.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}